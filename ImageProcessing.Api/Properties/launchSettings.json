{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Swagger": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger", // Or "swagger/index.html" or your custom route prefix
      "applicationUrl": "https://localhost:5292;http://localhost:7072",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "http://localhost:5292",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:7072;http://localhost:5292",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
